<manifest
	xmlns:ml="magicleap"
	ml:package="ndtech.magic.gl.sample"
	ml:version_code="1"
	ml:version_name="1.0">
	<application
		ml:visible_name="ndtechSample"
                ml:sdk_version="0.19.0"
                ml:min_api_level="3">
    <uses-privilege ml:name="LowLatencyLightwear" />
    <uses-privilege ml:name="PwFoundObjRead" />
    <uses-privilege ml:name="GesturesSubscribe" />
    <uses-privilege ml:name="GesturesConfig" />
    <uses-privilege ml:name="ControllerPose" />
		<component
			ml:name=".fullscreen"
			ml:visible_name="ndtechSample"
			ml:binary_name="bin/ndtechSample"
			ml:type="Fullscreen">
      <icon
        ml:model_folder="./ndtechSamplePortalIcon/out/icon/model"
        ml:portal_folder="./ndtechSamplePortalIcon/out/icon/portal" />
		</component>
	</application>
</manifest>
